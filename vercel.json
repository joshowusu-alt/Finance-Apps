{
    "buildCommand": "cd cashflow-app && npm run build",
    "outputDirectory": "cashflow-app/.next",
    "installCommand": "cd cashflow-app && npm install",
    "framework": "nextjs",
    "crons": [
        {
            "path": "/api/cron/keepalive",
            "schedule": "0 8 * * *"
        },
        {
            "path": "/api/cron/plaid-sync",
            "schedule": "0 6 * * *"
        }
    ]
}